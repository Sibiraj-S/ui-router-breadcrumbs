{
  "name": "ui-router-breadcrumbs",
  "version": "2.0.0",
  "description": "A Simple directive that creates breadcrumbs on the fly for AngularJs pages using angular-ui-router",
  "private": true,
  "author": "Sibiraj",
  "license": "MIT",
  "repository": "https://github.com/sibiraj-s/ui-router-breadcrumbs.git",
  "bugs": "https://github.com/sibiraj-s/ui-router-breadcrumbs/issues",
  "homepage": "https://github.com/sibiraj-s/ui-router-breadcrumbs#readme",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "serve": "grunt serve",
    "build": "grunt build",
    "lint:coffee": "grunt coffeelintr",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint": "npm run lint:coffee && npm run lint:js",
    "prepublishOnly": "node ./scripts/prepublish.js",
    "prerelease": "npm run build",
    "release": "sh ./scripts/release.sh",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "keywords": [
    "angular-breadcrumbs",
    "ui-router-breadcrumbs",
    "breadcrumbs",
    "ui-router",
